   "source": [
    "## 삽입정렬\n",
    "- 값의 양이 길어지면 효율 극악\n",
    "- 구현이 간단\n",
    "- 최대 O(n^2)의 시간 복잡도 -> for문 두개"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[8, 10, 2, 40, 3, 5, 7, 4]\n"
     ]
    }
   ],
   "source": [
    "a = [8,10,2,40,3,5,7,4]\n",
    "\n",
    "for i in range(0, len(a)-1):\n",
    "    for j in range(i, len(a)):\n",
    "        if a[i] > a[j]:\n",
    "            tmp = a[i]\n",
    "            a[i] = a[j]\n",
    "            a[j] = tmp\n",
    "        else: break\n",
    "print(a)"
   ]
  },
